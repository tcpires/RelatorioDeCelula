<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="200dp"
    android:layout_height="wrap_content"
    android:padding="8dp">

    <TextView
        android:id="@+id/tv_week_detail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/week_cell_item"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <TextView
        android:id="@+id/tv_members_detail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/members_cell_item"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_week_detail"
        android:layout_marginTop="8dp"/>
    <TextView
        android:id="@+id/tv_visitors_detail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/visitors_cell_item"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_members_detail"
        android:layout_marginTop="8dp"/>
    <TextView
        android:id="@+id/tv_regulars_detail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/regulars_cell_item"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_visitors_detail"
        android:layout_marginTop="8dp"/>


</androidx.constraintlayout.widget.ConstraintLayout>